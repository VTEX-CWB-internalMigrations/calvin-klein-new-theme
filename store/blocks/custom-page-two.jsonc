      }
    },

    "info-card#men-one-right":{
        "props": {
            "id": "info-card-main-home",
            "isFullModeStyle": false,
            "textPosition": "left",
            "imageUrl": "https://storecomponents.vteximg.com.br/arquivos/banner-infocard2.png",
            "headline": "Título",
            "subhead": "Texto informativo cadastrado aqui",
            "callToActionText": "Botão de ação",
            "callToActionUrl": "/sale/d",
